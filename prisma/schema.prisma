generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Evaluacion {
  id          Int      @id @default(autoincrement())
  nombre      String
  puntaje     Int
  maxPuntaje  Int
  porcentaje  Float
  resultado   String
  fecha       DateTime @default(now())
}
